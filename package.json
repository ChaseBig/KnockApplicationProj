{
  "name": "KnockApplicationProj",
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "clearMocks": true,
    "collectCoverage": true,
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "testMatch": [
      "<rootDir>/Tests/**/*.js",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/e2e"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy"
    },
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|glamorous-native)"
    ]
  },
  "version": "0.0.0",
  "description": "",
  "keywords": [
    "knock"
  ],
  "author": "Knock",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.2.2",
    "chai": "^4.2.0",
    "jasmine": "^3.3.1",
    "jasmine-fail-fast": "^2.0.0",
    "node": "^11.9.0",
    "react": "^16.0.0",
    "react-native": "0.51.1",
    "react-dom": "^16.0.0-beta.5",
    "react-cli": "^0.3.1",
    "react-native-render-html": "^3.10.0",
    "react-test-renderer": "^16.0.0-alpha.12"
  },
  "devDependencies": {
    "babel-jest": "21.2.0",
    "babel-preset-react-native": "4.0.0",
    "detox": "^10.0.5",
    "jest": "^21.2.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-15": "^1.3.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "enzyme-to-json": "^3.3.5",
    "eslint-plugin-detox": "^1.0.0",
    "identity-obj-proxy": "^3.0.0",
    "node": "^11.9.0",
    "react": "^16.5.0",
    "react-dom": "^16.0.0-beta.5",
    "react-cli": "^0.3.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-32.0.0.tar.gz",
    "react-native-render-html": "^3.10.0",
    "react-test-renderer": "^16.0.0-alpha.12"
  },
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test-ios": "detox build --configuration ios.sim.debug && detox test --configuration ios.sim.debug",
    "test-android": "detox build --configuration android.emu.debug && detox test --configuration android.emu.debug -l verbose",
    "test": "jest",
    "test:e2e": "detox test -c ios.sim.debug",
    "test:e2e:ios": "pushd detox/test && npm run e2e:ios && popd",
    "test:e2e:android": "pushd detox/test && npm run e2e:android && popd",
    "test:e2e:build": "detox build",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "release": "node scripts/release.js"
  },
  "detox": {
    "test-runner": "jest",
    "runner-config": "e2e/config.json",
    "configurations": {
      "ios.sim.release": {
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/DemoReactNativeJest.app",
        "build": "export RCT_NO_LAUNCH_PACKAGER=true && xcodebuild -project ios/DemoReactNativeJest.xcodeproj -UseNewBuildSystem=NO -scheme DemoReactNativeJest -configuration Release -sdk iphonesimulator -derivedDataPath ios/build -quiet",
        "type": "ios.simulator",
        "name": "iPhone 7 Plus"
      },
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/DemoReactNativeJest.app",
        "build": "xcodebuild -project ios/DemoReactNativeJest.xcodeproj -UseNewBuildSystem=NO -scheme DemoReactNativeJest -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 7 Plus"
      },
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "pushd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && popd",
        "type": "android.emulator",
        "name": "Nexus_5X_API_26"
      },
      "android.emu.release": {
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "pushd android && ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release && popd",
        "type": "android.emulator",
        "name": "Nexus_5X_API_26"
      }
    }
  }
}
